{
    "name": "jsondbphp/jsondb",
    "description": "JSON DB for APIs Testing for Your Minimalist Project",
    "type": "library",
    "keywords": ["jsondb", "jsondb-server", "database"],
    "require": {
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.7"
    },
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
